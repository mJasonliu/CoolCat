<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:tools="http://schemas.android.com/tools"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              tools:context="com.axis.fragment.OLMusicFragment"
              android:orientation="vertical"
              android:background="@drawable/bg"
    android:id="@+id/fragment_olmusic">

    <!-- TODO: Update blank fragment layout -->


    <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/strOlMusic_tSongName"
            android:id="@+id/olMusic_tWant"
            android:textSize="25sp"/>

    <EditText
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:id="@+id/olMusic_eWant"/>



    <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:text="@string/strOlMusic_bSearch"
        android:id="@+id/olMusic_bSearch" />
</LinearLayout>
